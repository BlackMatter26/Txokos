    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.17.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
